{
    "name": "axl-media/rememberable",
    "description": "Query caching for Laravel.",
    "keywords": ["laravel", "eloquent", "caching", "remember"],
    "license": "MIT",
    "homepage": "https://github.com/axl-media/rememberable",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwight@studiousapp.com"
        },
        {
            "name": "Alex Renoki",
            "email": "alex.g@axl.media"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AXLMedia\\Rememberable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AXLMedia\\Rememberable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~4.0.0",
        "orchestra/database": "~3.5.0|~3.6.0|~4.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
